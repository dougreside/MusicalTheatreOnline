div.panel {
	background: url('./images/bkgd_70black.png');
	padding: 5px;
	width:800px;
	position: absolute;
	/*
margin-top: 30px;
*/
	z-index: 300;
	
}
div.panel_InBckGrnd{
	background: url('./images/bkgd_70black.png');
	padding: 5px;
	width:800px;
	position: absolute;
	margin-top: 30px;
	z-index: 100;
	overflow: hidden;
}
.panelBody {
	position: relative;
	clear: left;
	overflow: scroll;
	height: 400px;
	background-color: white;

}
.ft {
	position: relative;
	color: #fff;
	
}
.hd {
	position: relative;
	text-align: right;
	height: 22px;
}
.panelButton{
	padding-left: 2px;
}
.registration {
	background-color: #FFF;
	
}
.regsubmit {
	background-color: #FFF;
	
}
.left padding {
	background-color: #FFF;
}

.zoomIn, .zoomOut, .darker, .lighter, .pageBack, .pageNext, .showImage, .showText, .magGlass, .dropDown {
	display: block;
	float: left;
	margin-right: 1px;
}

.pageBack{
	background-image: url("./images/icon_arrow_left.png");
	width: 20px;
	height: 16px;
}
.dropDown {
	font-size: 10px;
	position: relative;
	top: -2px;
	margin: 0 2px;
	/* for IE */
  	filter:alpha(opacity=65);
  	/* CSS3 standard */
  	opacity:0.65;
}
.pageNext{
	background-image: url("./images/icon_arrow_right.png");
	width: 20px;
	height: 16px;
}
.zoomIn{
	width: 20px;
	height: 16px;
	background-image: url("./images/icon_plus.png");
}
.zoomOut{
	width: 20px;
	height: 16px;
	background-image: url("./images/icon_minus.png");
}
.pin{
	display: block;
	float: right;
	margin-right: 1px;
	width: 20px;
	height: 16px;
	background: url("./images/btn_popin.png") no-repeat right;
}
.darker{
	width: 20px;
	height: 16px;
	background: url("./images/icon_plus.png") no-repeat right;
}
.lighter{
	width: 20px;
	height: 16px;
	background: url("./images/icon_minus.png") no-repeat right;	
}
.showImage {
	width: 20px;
	height: 16px;
	background: url("./images/icon_img.png") no-repeat right;
}
.showText {
	width: 20px;
	height: 16px;
	background: url("./images/icon_txt.png") no-repeat right;
}

.magGlass {
	width: 20px;
	height: 16px;
	background: url("./images/icon_magnify.png") no-repeat right;
}
.closeButton {
		display: block;
	float: right;
	margin-right: 1px;
	width: 20px;
	height: 16px;
	background: url("./images/icon_close.png") no-repeat right;
}
.cropButton{
	position: absolute;
	left: 0px;
	top: 0px;
	width:40px;
	height:40px;
	z-index: 999;
	background: url("./images/crop_image.png");
}
.pageControls, .zoomControls, .opacityControls, .imageToggle, .magControl {
	float: left;
	display: block;
	margin-right: 25px;
}

.infoBoxMain{
	background: none;
	color: #FFF;
	font-size: 13px;
	font-weight: bold;
	text-transform: uppercase;
	position: absolute;
	left: 228px;
	top: 28px;
}
.pageControls{
}
.zoomControls{
}

.opacityControls{	
	width: 100px;
}
.imageToggle{
}
.textLabelSpan {
	float: left;
	margin-right: 7px;
	color: #a9a9a9;
	font-size: 11px;
	line-height: 15px;
}
img{
	-moz-user-select: none;
}
