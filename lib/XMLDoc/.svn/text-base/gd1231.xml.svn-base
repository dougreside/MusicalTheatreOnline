<sp who="Andy" xml:id="GD1231826"><speaker xml:id="GD1231827">ANDY</speaker><lb xml:id="GD1231828"/>
<p xml:id="GD1231829"><stage xml:id="GD1231830">(to JACK)</stage><emph xml:id="GD1231831">My</emph> sister is not allowed to float <emph xml:id="GD1231832">anyone's</emph> boat!</p>  <lb xml:id="GD1231833"/>

<lb xml:id="GD1231834"/>
</sp><sp who="Jack" xml:id="GD1231835"><speaker xml:id="GD1231836">JACK</speaker><lb xml:id="GD1231837"/>
<p xml:id="GD1231838">Aww, you're no fun.</p> <lb xml:id="GD1231839"/>
<lb xml:id="GD1231840"/>
</sp><sp who="Skip" xml:id="GD1231841"><speaker xml:id="GD1231842">SKIP</speaker><lb xml:id="GD1231843"/>
