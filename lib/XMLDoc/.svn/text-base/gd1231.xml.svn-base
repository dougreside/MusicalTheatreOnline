<sp who="Will" xml:id="GD12312638"><speaker xml:id="GD12312639">WILL</speaker><lb xml:id="GD12312640"/>
<lg xml:id="GD12312641"><l xml:id="GD12312642">WHEN YOU WERE YOUNG</l><lb xml:id="GD12312643"/>
