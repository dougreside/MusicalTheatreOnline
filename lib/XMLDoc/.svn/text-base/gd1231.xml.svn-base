    <lb xml:id="GD1231792"/>
<sp who="Skip" xml:id="GD1231793"><speaker xml:id="GD1231794">SKIP</speaker><lb xml:id="GD1231795"/>
<p xml:id="GD1231796">That's good. <stage xml:id="GD1231797">(beat)</stage> Does she still ask about me? </p></sp><lb xml:id="GD1231798"/>
<lb xml:id="GD1231799"/>

<sp who="Andy" xml:id="GD1231800"><speaker xml:id="GD1231801">ANDY</speaker><lb xml:id="GD1231802"/>
