<lb xml:id="GD1231689"/>
<sp who="Will" xml:id="GD1231690"><speaker xml:id="GD1231691">WILL</speaker><lg xml:id="GD1231692"><l xml:id="GD1231693">NOW ARE YOU READY FOR  </l></lg></sp><lb xml:id="GD1231694"/>
