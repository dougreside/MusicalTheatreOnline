<lb xml:id="GD1231856"/>
<sp who="Andy" xml:id="GD1231857"><speaker xml:id="GD1231858">ANDY</speaker><lb xml:id="GD1231859"/>
<stage xml:id="GD1231860">(grabbing at SKIP and JACK)</stage><p xml:id="GD1231861">Come here! </p></sp><lb xml:id="GD1231862"/>
