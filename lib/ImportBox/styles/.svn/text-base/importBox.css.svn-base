
#importBox {
	display:block;
	position:absolute;
	margin:0;
   	padding:0;
	left: 25%;
	top: 20%;
	width:450px;
	z-index: 100;
}
.import {
	padding:0 0 0 11px;
	margin:10px;
	background:url("../images/browseBox-left.png") top left no-repeat;
}
.import .hd {
	margin:0;
	padding:0;
}
.import .hd .hdContents {
	background:url("../images/browseBox-right.png") top right no-repeat;
	margin:0;
	padding:8px 0 0.5em 0;
	_padding-top:0px;
}
.import .hd .hdContents .hdBox {
	padding:8px 0 30px 0;
	margin-top:-10px;
}
.import .hd .hdContents h3 {
	font-size: 13px;	
	color: #fff;
	float:left;
}
.import .hd .hdContents img {
	padding-left:3px;
}
.import .hd .hdContents .controls {	
	padding-right:10px;
	float:right;
}
.import .bd {
	margin:0;
	padding:0;
	clear:both;
}
.import .bd .bdContents {
	margin:0 0 0 -11px;
	padding:0 11px 0.5em;
	border:1px solid #4c4c4c;
	border-width:0 1px;
	background:#4c4c4c;	
}
.import .bd .bdContents .importArea {
	background: url('../images/bkgd_browse.jpg') no-repeat;
}
.import .bd .bdContents .importArea .importForm {
	padding-left:50px;
	padding-bottom:10px;
	height:40px;
}
.importForm label {
	color:#ffffff;
	font-size:11px;
	height:15px;
}
.import .bd .bdContents .importArea .importbox {
	 border-style: hidden;
     border-width: 0px;
     color: #3ea7a0;
     padding:8px 2px 5px 7px; 
     font-size:9pt;
	 width:221px;
     background: url('../images/importbox-2.gif') no-repeat;
	 background-repeat:no-repeat;
	 vertical-align:top;
}
input.importFile {
	margin-top:5px;	
	color:#3ea7a0;
}
.import .ft {
	margin:0;
	padding:0;
}
.import .ft .ftContents {
	margin:0 0 0 -11px;
	padding-left:11px;
	background:url("../images/browseBox-left.png") bottom left no-repeat;
}
.import .ft .ftContents span.corner {
	display:block;
	text-align:right;
	height:50px;
	padding:0 10px 10px 0;
	background:url("../images/browseBox-right.png") bottom right no-repeat;
	_margin-right:2px;
}
.import .ft .ftContents span.corner .uploadFiles img {
	padding-top:10px;
	float:right;
	z-index:2;
	clear:left;
}

