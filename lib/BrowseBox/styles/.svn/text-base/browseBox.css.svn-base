/* -- Yahoo CSS Reset ------------------------------------------------- */

body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td { 
	margin:0;
	padding:0;
}
table {
	border-collapse:collapse;
	border-spacing:0;
}
fieldset,img { 
	border:0;
}
address,caption,cite,code,dfn,em,strong,th,var {
	font-style:normal;
	font-weight:normal;
}
ol,ul {
	list-style:none;
}
caption,th {
	text-align:left;
}
h1,h2,h3,h4,h5,h6 {
	font-size:100%;
	font-weight:normal;
}
q:before,q:after {
	content:'';
}
abbr,acronym { border:0;
}

/* general styles */

body {
	font-family: "Lucida Grande", Verdana, Arial, sans-serif;
	font-size: 11px;
	margin:0;
   	padding:0;
}

.browseBox {
	position:absolute;
	margin:0;
   	padding:0;
	width:800px;
}

.browse {
	padding:0 0 0 11px;
	margin:10px;
	background:url("../images/browseBox-left.png") top left no-repeat;
}

.browse .hd {
	margin:0;
	padding:0;
}

.browse .hd .hdContents {
	background:url("../images/browseBox-right.png") top right no-repeat;
	margin:0;
	padding:8px 0 0.5em 0;
}

.browse .hd .hdContents .hdBox {
	padding:8px 0 30px 0;
	margin-top:-10px;
}

.browse .hd .hdContents h3 {
	font-size: 13px;	
	color: #fff;
	float:left;
}

.browse .hd .hdContents img {
	padding-left:3px;
}

.browse .hd .hdContents .controls {	
	padding-right:10px;
	float:right;
}

.browse .bd {
	margin:0;
	padding:0;
	clear:both;
}

.browse .bd .bdContents {
	margin:0 0 0 -11px;
	padding:0 11px 0.5em;
	border:1px solid #4c4c4c;
	border-width:0 1px;
	background:#4c4c4c;	
}

.browse .bd .bdContents .searchArea {
	background: url('../images/bkgd_browse.jpg') no-repeat;
}
.browse .bd .bdContents .searchArea h3 {
	color:#fff;
	font-size:11px;
	height:15px;
	padding-bottom:8px;
	padding-left:40px;
}
.browse .bd .bdContents .searchArea .search {
	padding-left:50px;
	padding-bottom:10px;
}

.browse .bd .bdContents .searchArea .searchbox {
	 border-style: hidden;
     border-width: 0px;
     color: #3ea7a0;
     padding:8px 2px 5px 7px;
	 
     font-size:9pt;
	 
	 width:221px;
     background: url('../images/searchbox-2.gif') no-repeat;
	 background-repeat:no-repeat;
	 vertical-align:top;
}


.browse .ft {
	margin:0;
	padding:0;
}

.browse .ft .ftContents {
	margin:0 0 0 -11px;
	padding-left:11px;
	background:url("../images/browseBox-left.png") bottom left no-repeat;
}
.browse .ft .ftContents span.corner {
	display:block;
	text-align:right;
	height:50px;
	padding:0 10px 10px 0;
	background:url("../images/browseBox-right.png") bottom right no-repeat;
}


.browse .ft .ftContents span.corner .openFiles img {
	padding-top:10px;
	float:right;
	z-index:2;
}


div.tableContainer {
	border: solid #aeaeae;
    border-width: 0px 0px 1px 1px;
	clear: both;
	overflow: hidden;
	width: 756px;
	height:283px;
	position:relative;
}

table.scrollTable {
	width:100%;
}

thead.fixedHeader tr {
	display: block;	
}

thead.fixedHeader th {
	background: #fff url('../images/bkgd_browseList.jpg') repeat-x;
	font-weight:bold;
	height:21px;
	margin:0;
	padding: 2px 3px 3px 4px;
	text-align: left;
}

thead.fixedHeader a, thead.fixedHeader a:link, thead.fixedHeader a:visited {
	color:#4A4A4A;
	display: block;
	text-decoration: none;
}

thead.fixedHeader a:hover {
	text-decoration: underline;
}

tbody.scrollContent {
	display: block;
	height: 262px;
	overflow: auto;
}

tbody.scrollContent tr {
	line-height:15px;
	height:15px;
	margin:0;	
}

tbody.scrollContent td, tbody.scrollContent tr.normalRow td {
	background: #FFF;
	border: solid #cccccc;
    border-width: 0px 1px 1px 0px;
	padding: 2px 3px 3px 4px;
	line-height:15px;
	height:15px;
	margin:0;
}

tbody.scrollContent tr.alternateRow td {
	background: #e2f6fd;
}

thead.fixedHeader th {
	width: 50px;
}

thead.fixedHeader th + th {
	width: 100px;
}

thead.fixedHeader th + th + th {
	width: 100px;
}

thead.fixedHeader th + th + th + th {
	width: 150px;
}

thead.fixedHeader th + th + th + th + th {
	width: 100px;
}

thead.fixedHeader th + th + th + th + th + th {
	width: 100px;
}

thead.fixedHeader th + th + th + th + th + th + th {
	width: 156px;
}

tbody.scrollContent td {
	width: 50px;
}

tbody.scrollContent td + td {
	width: 100px;
}

tbody.scrollContent td + td + td {
	width: 100px;
}

tbody.scrollContent td + td + td + td {
	width: 150px;
}

tbody.scrollContent td + td + td + td + td  {
	width: 100px;
}

tbody.scrollContent td + td + td + td + td + td {
	width: 100px;
}

tbody.scrollContent td + td + td + td + td + td + td {
	width: 156px;
}

