    <lb xml:id="fringe_1008"/>
    <sp who="Will" xml:id="fringe_1009"><speaker xml:id="fringe_1010">WILL</speaker><lb xml:id="fringe_1011"/>
<p xml:id="fringe_1012">Matt Hoffman, Chris Peterson, Eric Spencer, Danny Barwick - </p></sp><lb xml:id="fringe_1013"/>
