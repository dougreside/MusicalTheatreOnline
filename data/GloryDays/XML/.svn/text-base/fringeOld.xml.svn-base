<lb xml:id="fringe_1306"/>
<sp who="Will" xml:id="fringe_1307"><speaker xml:id="fringe_1308">WILL</speaker><lb xml:id="fringe_1309"/>
<p xml:id="fringe_1310">Cause she's...</p></sp><lb xml:id="fringe_1311"/>
<lb xml:id="fringe_1312"/>
