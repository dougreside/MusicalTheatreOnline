    <sp who="Jack" xml:id="fringe_1230"><speaker xml:id="fringe_1231">JACK</speaker><lb xml:id="fringe_1232"/>
        <p xml:id="fringe_1233"> Really!? Did you?</p></sp><lb xml:id="fringe_1234"/>

    <lb xml:id="fringe_1235"/>
    <sp who="Andy" xml:id="fringe_1236"><speaker xml:id="fringe_1237">ANDY</speaker><lb xml:id="fringe_1238"/>
        <p xml:id="fringe_1239">Yeah, he took her to Denny's.</p></sp> <lb xml:id="fringe_1240"/>
    <lb xml:id="fringe_1241"/>
    <sp who="Skip" xml:id="fringe_1242"><speaker xml:id="fringe_1243">SKIP</speaker><lb xml:id="fringe_1244"/>
        <p xml:id="fringe_1245">Classy.</p></sp><lb xml:id="fringe_1246"/>

    <lb xml:id="fringe_1247"/>
    <sp who="Will" xml:id="fringe_1248"><speaker xml:id="fringe_1249">WILL</speaker><lb xml:id="fringe_1250"/>
        <p xml:id="fringe_1251">Thank.</p></sp><lb xml:id="fringe_1252"/>
    <lb xml:id="fringe_1253"/>
<sp who="Andy" xml:id="fringe_1254"><speaker xml:id="fringe_1255">ANDY</speaker><lb xml:id="fringe_1256"/>
    <p xml:id="fringe_1257">- and lemme tell you that girl can THROW DOWN! She got a T- Bone steak with onion rings -</p></sp><lb xml:id="fringe_1258"/>
    
    
