<sp who="Jack" xml:id="joespub_062212"><speaker xml:id="joespub_062213">JACK</speaker><lb xml:id="joespub_062214"/>

<p xml:id="joespub_062215">The password has to be written down somewhere, right?</p></sp><lb xml:id="joespub_062216"/>
 <lb xml:id="joespub_062217"/>
<sp who="Skip" xml:id="joespub_062218"><speaker xml:id="joespub_062219">SKIP</speaker><lb xml:id="joespub_062220"/>
